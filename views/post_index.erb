<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
  <h2>Posts</h2>
  <ul>
    <% @posts.each do |post| %>
     
               <li> 
               <a href= "/posts/<%= post.id%>/show"><%= post.f_name %> <%= post.l_name %></a>
                
                <img src="<%= post.photo_url %>"/>
                <%= post.description %>
                
                   <a href="/posts/<%= post.id %>/edit">Edit</a>
                    <form action="/posts/<%= post.id %>" method="POST">
                      <input type="hidden" name="_method" value="DELETE">
                      <button>Delete</button>
                    </form>
                </li>

    <% end %>
  </ul>
</body>
</html> 